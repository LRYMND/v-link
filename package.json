    "electron-typescript-ipc": "^3.0.0",
    "events": "^3.3.0",
    "jmuxer": "^2.0.5",
    "node-carplay": "github:rhysmorgan134/node-CarPlay",
    "pcm-player": "^0.0.16",
    "pcm-ringbuf-player": "^0.0.3",


    "socketmost": "^2.0.5",
    "stream-browserify": "^3.0.0",
    "usb": "^2.10.0",
    "vite-plugin-esmodule": "^1.5.0",
    "socketcan": "^4.0.4",

    "electron-json-storage": "^4.6.0",
    "python-shell": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-simple-keyboard": "^3.5.3",
    "react-spinners-kit": "^1.9.1",
